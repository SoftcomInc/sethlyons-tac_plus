group = <%= @group %> {
<% if !@default_service.empty? -%>
    default service = <%= @default_service %>
<% end -%>
<% @service.each do |srv, srv_opts| -%>
<% if !srv_opts.empty? -%>
    service = <%= srv %> {
<% srv_opts.each do |service_opts| -%>
      <%= service_opts %>
<% end -%>
    }
<% else -%>
    service = <%= srv %>
<% end -%>
<% end -%>
<% @protocol.each do |proto, proto_opts| -%>
<% if !proto_opts.empty? -%>
    protocol = <%= proto %> {
<% proto_opts.each do |proto_options| -%>
      <%= proto_options %>
<% end -%>
    }
<% else -%>
    protocol = <% proto %>
<% end -%>
<% end -%>
<% @cmd.each do |comd, comd_opts| -%>
    cmd = <%= comd %> {
<% comd_opts.each do |cmd_opts| -%>
      <%= cmd_opts %>
<% end -%>
    }
<% end -%>
<% if !@additional_attrs.nil? -%>
    <%= @additional_attrs %>
<% end -%>
}

